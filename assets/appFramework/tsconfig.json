    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "jsx": "react",
        "typeRoots": [
            "./customTypes",
            "./node_modules/@types"
        ]
    }
}