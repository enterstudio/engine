{
    "font-awesome": {
        "name": "font-awesome",
        "includeMode": "both",
        "assets": ["fonts/**/*"]
    },
    "bootstrap4": {
        "name": "bootstrap",
        "version": "4.0.0-beta",
        "includeMode": "both",
        "noScript": true
    },
    "bluebird": {
        "name": "bluebird",
        "main": "/js/browser/bluebird.js",
        "mainMinified": "/js/browser/bluebird.min.js",
        "includeMode": "both",
        "scriptIncludeMode": "both"
    },
    "prismjs": {
        "name": "primsjs",
        "main": "prism.js",
        "includeMode": "both",
        "assets": ["components/**/*.min.js"]
    },
    "rxjs": {
        "name": "rxjs",
        "main": "*"
    }
}