{
    "parser": "espree",
	"parserOptions": {
		"ecmaVersion": 3
	},
	"extends": [
		"eslint:all"
	],
	"env": {
	},
	"globals": {},
	"rules": {
        "no-var": 0,
        "one-var": 0,
        "array-bracket-newline": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "function-paren-newline": 0,
        "padded-blocks": 0,
        "array-callback-return": 0,
        "no-magic-numbers": 0,
        "consistent-return": 0,
        "require-jsdoc": 0,
        "func-style": 0,
        "vars-on-top": 0,
        "promise/no-nesting": 0,
        "promise/always-return": 0,
        "dot-location": 0,
        "no-console": 0
	},
	"plugins": [
	]
}